<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8">
    <title>E Store - eCommerce HTML Template</title>
    <meta content="width=device-width, initial-scale=1.0" name="viewport">
    <meta content="eCommerce HTML Template Free Download" name="keywords">
    <meta content="eCommerce HTML Template Free Download" name="description">

    <!-- Favicon -->
    <link href="/Assets/Customer/img/favicon.ico" rel="icon">

    <!-- Google Fonts -->
    <link href="https://fonts.googleapis.com/css?family=Open+Sans:300,400|Source+Code+Pro:700,900&display=swap" rel="stylesheet">

    <!-- CSS Libraries -->
    <link href="https://stackpath.bootstrapcdn.com/bootstrap/4.4.1/css/bootstrap.min.css" rel="stylesheet">
    <link href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/5.10.0/css/all.min.css" rel="stylesheet">
    <link href="/Assets/Customer/lib/slick/slick.css" rel="stylesheet">
    <link href="/Assets/Customer/lib/slick/slick-theme.css" rel="stylesheet">
   

    <!-- Template Stylesheet -->
    <link href="/Assets/Customer/css/style.css" rel="stylesheet">

</head>

<body>
    @RenderSection("jsHeader", required: false)
    <!-- Top bar End -->
    <!-- Nav Bar Start -->

    @{Html.RenderAction("TopMenu", "Home");}
    <!-- Nav Bar End -->
    <!-- Bottom Bar Start -->
    @{ Html.RenderAction("HeaderSave", "Home");}

    @{Html.RenderAction("MainMenu", "Home");}
    <!-- Bottom Bar End -->
    <!-- Main Slider Start -->
    @RenderSection("slide", required: false)
    <!-- Main Slider End -->
    <!-- Brand Start -->
    @RenderBody()

    <!-- Review End -->
    <!-- Footer Start -->
    @{Html.RenderAction("Footer", "Home");}
    <!-- Footer Bottom End -->
    <!-- Back to Top -->
    <a href="#" class="back-to-top"><i class="fa fa-chevron-up"></i></a>

    <!-- JavaScript Libraries -->
    <script src="https://code.jquery.com/jquery-3.4.1.min.js"></script>
    <script src="https://stackpath.bootstrapcdn.com/bootstrap/4.4.1/js/bootstrap.bundle.min.js"></script>
    <script src="/Assets/Customer/lib/easing/easing.min.js"></script>
    <script src="/Assets/Customer/lib/slick/slick.min.js"></script>

    <!-- Template Javascript -->
    <script src="/Assets/Customer/js/main.js"></script>
    <script src="/Assets/Client/js/controller/baseController.js"></script>

    @RenderSection("jsFooter", required: false)
</body>
</html>

